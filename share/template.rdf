<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:rednet="http://redgates.com/2015/5/rednet"
    xmlns:fn="http://www.w3.org/2005/02/xpath-functions"
    xmlns:xsd="http://www.w3.org/2001/XSL/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XSL/XMLSchema-instance">

  <owl:Ontology rdf:about="http://redgates.com/2015/5/rednet">
    <dc:title>The Rednet Cloud Switch Configuration Vocabulary</dc:title>
    <dc:description>
    RDF schema for rednet cloud switch configuration
    </dc:description>
    <dc:creator>Karl Redgate</dc:creator>
    <dc:date>2015-05-08</dc:date>
  </owl:Ontology>


  <rednet:CA rdf:about='http://redgates.com/2015/5/rednet/CA'>
    <dc:description>
    The certificate authority certificate.
    </dc:description>
    <rednet:certificate></rednet:certificate>
  </rednet:CA>

  <rednet:Client rdf:about='http://redgates.com/2015/5/rednet/Client'>
    <dc:description>
    The signed client certificate.
    </dc:description>
    <rednet:certificate></rednet:certificate>
  </rednet:Client>

  <rednet:DH rdf:about='http://redgates.com/2015/5/rednet/DH'>
    <dc:description>
    The Diffie-Hellman key to use for key exchange.
    </dc:description>
    <rednet:key></rednet:key>
  </rednet:DH>

  <rednet:SSH rdf:about='http://redgates.com/2015/5/rednet/SSH'>
    <dc:description>
    The ssh public key for access to appliance.
    The private key is ONLY present within Carbonite.
    </dc:description>
    <rednet:key></rednet:key>
  </rednet:SSH>

  <rednet:Server rdf:about='http://redgates.com/2015/5/rednet/Server'>
    <dc:description>
    The IP address of the rednet server.
    </dc:description>
    <rednet:address></rednet:address>
  </rednet:Server>

</rdf:RDF>
